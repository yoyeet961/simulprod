function onAddComponent(n,t,i){var r=$("#"+i),u=$("#"+i+"> button");u.html('<span class="glyphicon glyphicon-minus" style="line-height: 0;"><\/span>');u.removeClass("btn-success");u.addClass("btn-danger");u.attr("onclick","onRemoveComponent("+n+", '"+t+"' ,"+i+")");r.append("<input name='podComponents.Index' value='"+i+"'class='hidden'>");r.append("<input name='podComponents["+i+"].componentId'  value= '"+n+"' class='hidden'>");r.append("<input name='podComponents["+i+"].Type'  value= '"+t+"' class='hidden'>");$("#delete-List").append(r)}function onRemoveComponent(n,t,i){var f=$("#"+i),r=$("#"+i+"> button");r.html("Add");r.removeClass("btn-danger");r.addClass("btn-success");r.attr("onclick","onAddComponent("+n+", '"+t+"' ,"+i+")");$("#"+i+" > input").remove();$("#add-List").append(f);var u=$("#delete-List").children(),e=u.map(n=>u[n].id).toArray(),o=e.filter(n=>n!=i),s=originalAddList.filter(function(){return!o.includes(this.id)});$("#add-List").html(s)}function showTab(n){var t=$(".tab");t[n].style.display="block";n==t.length-1?$("#nextBtn").html("Submit"):$("#nextBtn").html("Next");fixStepIndicator(n);n==2&&fixComponentList()}function fixComponentList(){var t=$("#teamsDropDown option:selected").text(),i=$("#podsDropDown option:selected").text(),n;$("#add-TitleCard").html("Components Owned by (Selected) Team: <strong>"+t+"<\/strong>, and Pod: <strong>"+i+"<\/strong>");n=$("#systemName").val();$("#remove-TitleCard").html("Components to be added to the <strong>'"+n+"'<\/strong> System");var r=$("#teamsDropDown").val(),u=$("#podsDropDown").val(),f=$("#add-List > div").filter(function(){return this.dataset.podid==u&&this.dataset.teamid==r});$("#add-List").html(f);originalAddList=$("#add-List").children()}function nextPrev(n){var t=$(".tab");if(n==1&&!validateForm())return!1;if(t[currentTab].style.display="none",currentTab=currentTab+n,currentTab>=t.length)return $("#stepsAndSpinner > span").each(function(){this.classList.add("hidden")}),$("#stepsAndSpinner").append('<p>One Moment Please... <\/p> <div class="loader"><\/div>'),$("#nextBtn").remove(),$("#sys-submit").click(),!1;showTab(currentTab)}function validateForm(){var n=!0;return $(".tab:eq("+currentTab+") > p > input").each(function(){this.value==""&&this.name!="SystemPageLink"&&(this.classList.add("invalid"),n=!1)}),currentTab!=2&&$(".tab:eq("+currentTab+") > textarea").each(function(){this.value==""&&(this.classList.add("invalid"),n=!1)}),n&&$(".step")[currentTab].classList.add("finish"),n}function fixStepIndicator(n){var t=$(".step").removeClass("active");t.removeClass("active");t[n].classList.add("active")}var currentTab=0;$(document).ready(function(){showTab(currentTab)});CreateSystemOwnerShip=function(){var n=function(n){return function(){var u=$("#teamsDropDown").val(),t=n,r="";for(i=0;i<t.length;i++)u==t[i].TeamID&&(r+="<option value="+t[i].ID+">"+t[i].Name+"<\/option>");$("#podsDropDown").html(r)}};return{teamDropDownClickedBuilder:n}}()