$(document).ready(function(){var n=function(n,t){var i=$(n).data("name"),r=t.toLowerCase(),f=prompt("Are you sure you want to delete "+r+' "'+i+'"? \nIf so, type YES.'),e=f==="YES";if(e){var o=$(n).data("url"),s=$(n).data("redirect-url"),u=$.post(o);u.done(function(n){n?alert(n):(alert(t+' "'+i+'" was successfully deleted'),window.location.href=s)});u.fail(function(){alert("Failed to delete "+r+' "'+i+'"')})}return!1};$("#delete-component-button").click(function(){return n(this,"Component")});$("#delete-team-button").click(function(){return n(this,"Team")});$("#delete-pod-button").click(function(){return n(this,"Pod")})});$(document).ready(function(){Roblox.select2helpers.setupStandardSelect2s({theme:"bootstrap",width:"100%"})})