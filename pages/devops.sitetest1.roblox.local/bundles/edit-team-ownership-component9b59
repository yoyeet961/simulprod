$(document).ready(function(){var n=function(n,t){var i=$(n).data("name"),r=t.toLowerCase(),f=prompt("Are you sure you want to delete "+r+' "'+i+'"? \nIf so, type YES.'),e=f==="YES";if(e){var o=$(n).data("url"),s=$(n).data("redirect-url"),u=$.post(o);u.done(function(n){n?alert(n):(alert(t+' "'+i+'" was successfully deleted'),window.location.href=s)});u.fail(function(){alert("Failed to delete "+r+' "'+i+'"')})}return!1};$("#delete-component-button").click(function(){return n(this,"Component")});$("#delete-team-button").click(function(){return n(this,"Team")});$("#delete-pod-button").click(function(){return n(this,"Pod")})});$(document).ready(function(){Roblox.select2helpers.setupStandardSelect2($("#TeamID"),{theme:"bootstrap",width:"100%"});Roblox.select2helpers.setupStandardSelect2($("#PodID"),{theme:"bootstrap",width:"100%"});Roblox.select2helpers.setupStandardSelect2($("#SystemId"),{theme:"bootstrap",width:"100%"});$("input.form-control").focus(Roblox.select2helpers.disableKeyboardShortcuts);$("input.form-control").blur(Roblox.select2helpers.enableKeyboardShortcuts)});var goToLink=function(n){window.open($(n).val(),"_blank")};typeof EditTeamOwnership=="undefined"&&(EditTeamOwnership={});EditTeamOwnership=function(){"use strict";var n={},t={},i=function(t){n=t;var i=$("#TeamID");i.on("change",function(){var i=n[this.value?this.value:0],u,f,e,r,t,o,s;if(i){r=$("#PodID");t="<option>[ Select pod ]<\/option>";for(u in i)i.hasOwnProperty(u)&&(f=i[u],e='<option value="'+f.Id+'">'+f.Name+"<\/option>",t+=e);r.html(t)}else r=$("#PodID"),t="<option>[ Select pod ]<\/option>",r.html(t);o=$("#SystemId");s="<option value=''>[ No System ]<\/option>";o.html(s)})},r=function(n){t=n;var i=$("#PodID");i.on("change",function(){var n=t[this.value?this.value:0],e=$("#SystemId"),u="<option value=''>[ No System ]<\/option>",i,r,f;if(n)for(i in n)n.hasOwnProperty(i)&&(r=n[i],f='<option value="'+r.Id+'">'+r.Name+"<\/option>",u+=f);e.html(u)})};return{initializePods:i,initializeSystems:r}}()