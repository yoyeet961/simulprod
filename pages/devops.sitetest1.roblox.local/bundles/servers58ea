function searchServers(){var i=$("#search-servers-input").val(),t,n;return i.length===0?!1:(t=$("#search-servers-status option:selected").text(),n="/Servers/SearchServers?searchQuery="+encodeURI(i),t.toLowerCase()!=="any"&&(n=n+"&serverStatus="+encodeURI(t)),$.get(n,function(n){$("#search-servers-table-results").html(n)}),!1)}var url,questionMarkIndex,queryStringParameters,queryStringParameterValues,i,searchServersText,serverStatusValue,serverStatusSelect,option;if($("select.filter").change(function(){var i=$("#mainTable tbody").find("tr").hide(),n=$("#HardwareClasses option:selected").val(),t;location.pathname!=="/Servers/ReadyForAllocation"?(t=$("#PublicNetworks option:selected").val(),t||n||i.show(),t&&n&&(i.hide(),$("."+t+"."+n).show()),t&&!n&&(i.hide(),$("."+t).show()),!t&&n&&(i.hide(),$("."+n).show())):(n&&(i.hide(),$("."+n).show()),n||i.show())}),url=window.location.href,questionMarkIndex=url.indexOf("?"),questionMarkIndex>0)for(queryStringParameters=url.substr(questionMarkIndex+1),queryStringParameterValues=queryStringParameters.split("&"),i=0;i<queryStringParameterValues.length;i++)if(queryStringParameterValues[i].lastIndexOf("searchQuery=",0)===0)searchServersText=document.getElementById("search-servers-input"),searchServersText.value=queryStringParameterValues[i].substr(12);else if(queryStringParameterValues[i].lastIndexOf("serverStatus=",0)===0){serverStatusValue=queryStringParameterValues[i].substr(13);serverStatusSelect=document.getElementById("search-servers-status");for(index in serverStatusSelect.options)if(option=serverStatusSelect.options[index],option.value.toLowerCase()===serverStatusValue){serverStatusSelect.selectedIndex=index;break}}searchServers();$("#help").click(function(){$("#help-details").slideToggle()})